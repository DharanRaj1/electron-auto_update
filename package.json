{
    "name": "electron-auto-update-test",
    "version": "1.0.0",
    "description": "Test app for Electron auto-update via GitHub",
    "main": "main.js",
    "scripts": {
      "start": "electron .",
      "pack": "electron-builder --dir",
      "dist": "electron-builder"
    },
    "build": {
      "appId": "com.example.autoupdatetest",
      "publish": [
        {
          "provider": "github",
          "owner": "your-github-username",
          "repo": "your-repo-name"
        }
      ]
    },
    "dependencies": {
      "electron-updater": "^5.3.0"
    },
    "devDependencies": {
      "electron": "^28.2.0",
      "electron-builder": "^24.8.0"
    }
  }
  